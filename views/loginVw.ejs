<body>
<div class="container-fluid" style="box-sizing: border-box;">
    <div class="row">
        <div class="col-md-8" style="background-color: #00c8eb; height: 100vh; padding-top: 8%;">
           <img src="/images/png-login.png"width="800px" alt="tokodigital">
        </div>
        <div class="col-md-4" style="background-color: #00c8eb;height: 100vh;">
            <div class="card" style="margin-top: 20%; border-radius: 14px; height: 500px;">
                <div class="card-header">
                    <br>
                    <h2 class="card-tittle text-center">Selamat Datang Admin</h2>
                    <h5 class="text-center">Silahkan login untuk melanjutkan</h5>
                </div>
                <form action="/routes/login.js" method="post">  
                    <div class="card-body">
                            <h6>Username :</h6>
                            <input type="text" class="form-control" placeholder="Silahkan Masukan Username" aria-label="Username">
                            <br>
                            <h6>Password :</h6>
                            <input type="password" class="form-control" placeholder="Silahkan Masukan Password" aria-label="Password">
                            <br>
                            <div class="checkbox">
                                <label><input type="checkbox" value=""> Remember Me</label>
                            </div>
                            <br>    
                            <div class="d-grid gap-2 col-8 mx-auto">
                                <button class="btn btn-primary" type="button">Login</button>
                            </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
  


   
